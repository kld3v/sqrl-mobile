{
  "name": "qrlaApp",
  "displayName": "qrlaApp",
  "expo": {
    "owner": "qrlalabs",
    "name": "qrlaApp",
    "slug": "qrlaApp",
    "scheme": "qrlaapp",
    "version": "2.1.1",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon-all.png",

    "extra": {
      "eas": {
        "projectId": "24d5ead5-ef54-41ac-a1ad-72e30e2e11c4"
      }
    },
    "splash": {
      "image": "./assets/images/splash-logo-all.png",
      "resizeMode": "contain",
      "backgroundColor": "#041522"
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#041522"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "jsEngine": "hermes",
    "assetBundlePatterns": ["**/*"],
    "android": {
      "versionCode": 6,
      "googleServicesFile": "./google-services.json",
      "icon": "./assets/images/app-icon-android-legacy.png",
      "package": "com.qrlaapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-icon-android-adaptive-foreground.png",
        "backgroundImage": "./assets/images/app-icon-android-adaptive-background.png"
      },
      "splash": {
        "image": "./assets/images/splash-logo-android-universal.png",
        "resizeMode": "contain",
        "backgroundColor": "#041522"
      }
    },
    "ios": {
      "buildNumber": "6",
      "icon": "./assets/images/app-icon-ios.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.qrlaapp",
      "config": {
        "usesNonExemptEncryption": false
      },
      "splash": {
        "image": "./assets/images/splash-logo-ios-mobile.png",
        "tabletImage": "./assets/images/splash-logo-ios-tablet.png",
        "resizeMode": "contain",
        "backgroundColor": "#041522"
      }
    },
    "web": {
      "favicon": "./assets/images/app-icon-web-favicon.png",
      "splash": {
        "image": "./assets/images/splash-logo-web.png",
        "resizeMode": "contain",
        "backgroundColor": "#041522"
      },
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#ffffff",
          "sounds": ["./assets/sounds/notification.wav", "./assets/sounds/notification.wav"]
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Qrla to use your location.",
          "locationAlwaysPermission": "Allow Qrla to use your location",
          "locationWhenInUsePermission": "Allow Qrla to use your location"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ],
      "expo-localization",
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": false
          },
          "android": {
            "newArchEnabled": false
          }
        }
      ]
    ],
    "experiments": {
      "tsconfigPaths": true
    }
  },
  "ignite": {
    "version": "9.4.2"
  }
}
